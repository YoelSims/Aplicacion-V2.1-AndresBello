{	
	"QtBinding": "PySide6",	
	"ShowLogs": true,
	"LiveCompileQss": true,
	"CheckForMissingicons": false,
	"QMainWindow":[
		{
			"tittle":"Sistema de Gestion Estudiantil AndresBello",
			"icon":":/imagenes/Imagenes/Logo/2.png",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "dimensionBarra",
			"navigation":[
				{
					"minimize":"minimizarBoton",
					"close": "cerrarBoton",
					"restore":[
						{	
							"buttonName": "restaurarBoton",
							"normalIcon": ":/font_awesome_regular/icons/font_awesome/regular/square.png",
							"maximizedIcon": ":/font_awesome_regular/icons/font_awesome/regular/clone.png"
						}
					],
					"moveWindow": "cabeceraContenedor",
					"tittleBar": "cabeceraContenedor"
				}
			]
		}
	],	
	"QCustomSlideMenu": [
		{
			"name": "menuContenedorIzquierdo",
			"defaultSize": [{
				"width": 48,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 48,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 145,
				"height": "parent"
			}],
			"toggleButton": [{
				"buttonName": "menuBoton",
				"icons": [{
					"whenMenuIsCollapsed": ":/font_awesome_solid/icons/font_awesome/solid/align-justify.png",
					"whenMenuIsExpanded": ":/font_awesome_solid/icons/font_awesome/solid/angles-left.png"
				}]
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 1200,
				"animationEasingCurve": "OutInBack",
				"whenCollapsing": [{
					"animationDuration": 400,
					"animationEasingCurve": "OutInBack"
				}],
				"whenExpanding": [{
					"animationDuration": 400,
					"animationEasingCurve": "Linear"
				}]
			}],
			"menuContainerStyle": [{
				"whenMenuIsCollapsed": [
					"border: 1px solid transparent"
				],
				"whenMenuIsExpanded": [
					"border: 1px solid transparent"
				]
			}],
				"floatPosition": [
					{
						"relativeTo": "centralContenedor",
						"position": "center-left",
						"shadow":[
							{
								"color": "#f00",
								"blurRadius": 7,
								"xOffset": 3,
								"yOffset": 0
							}
						]
					}
				]
			},
			{
			"name": "menuCentroContenedor",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 280,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 350,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "menuContenedorDerecho",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 250,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 350,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "emergenteNotificacionContenedor",
			"defaultSize": [{
				"width": 0,
				"height": "auto"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "auto"
			}],
			"expandedSize": [{
				"width": 800,
				"height": 100
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 300,
				"animationEasingCurve": "Linear"
			}],
			"floatPosition": [
				{
					"relativeTo": "centralContenedor",
					"position": "center-center",
					"shadow":[
						{
							"color": "#f00",
							"blurRadius": 30,
							"xOffset": 0,
							"yOffset": 0
						}
					]
				}
			],
			"toggleButton": [{
				"buttonName": "notificacionBoton",
				"icons": [{
					"whenMenuIsCollapsed": ":/font_awesome_regular/icons/font_awesome/regular/bell.png",
					"whenMenuIsExpanded": ":/font_awesome_regular/icons/font_awesome/regular/bell-slash.png"
				}]
			}]
		}
	],
	"QSettings": [
		{
			"ThemeSettings": [
				{
					"CustomTheme": [
						{
							"Background-color": "#ffffff",   
							"Text-color": "#000",         
							"Accent-color": "#ff0000",       
							"Theme-name": "Original",
							"Icons-color": "#ff0000",
							"Default-Theme": true            
						},
						{
                            "Background-color": "#16191d",
                            "Text-color": "#fff",
                            "Accent-color": "#03C3C3",
                            "Theme-name": "Negro-Azul",
                            "Default-Theme": false
                        },
                        {
                            "Background-color": "#fff",
                            "Text-color": "#070808",
                            "Accent-color": "#0F9696",
                            "Theme-name": "Blanco-Azul",
                            "Default-Theme": false,
                            "Icons-color": "#0F6464"
                        },
                        {
                            "Background-color": "#16191d",
                            "Text-color": "#fff",
                            "Accent-color": "#ff0000",
                            "Theme-name": "Negro-Rojo"
                        },
                        {
                            "Background-color": "#021924",
                            "Text-color": "#d9dde0",
                            "Accent-color": "#02d3c1",
                            "Theme-name": "Negro-Noche"
                        }
					]
				}
			],
			"AppSettings": {
				"OrganizationName": "Complejo Educativo Andres Bello",
				"ApplicationName": "Sistema Gestion Estudiantil Andres Bello",
				"OrginizationDormain": "AndresBello.org"
			}
		}
	],
	"QCustomQStackedWidget":[
		{
			"name":"principalPaginas",
			"transitionAnimation":[
				{
					"fade":[
						{
							"active": true,
							"duration":350,
							"easingCurve": "OutBack"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 350,
							"direction": "horizontal",
							"easingCurve": "Linear"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
							"inicioBoton": "inicioPagina",
							"estudianteBoton": "estudiantesPagina",
							"materiaBoton": "materiasPagina",
							"profesorBoton": "profesoresPagina",
							"notaBoton": "notasPagina",
							"documentoBoton": "documentosPagina",
							"gradosBoton": "gradosPagina"
						}
					]
				}
			]
		},
		{
			"name":"centroPaginas",
			"transitionAnimation":[
				{
					"fade":[
						{
							"active": true,
							"duration":350,
							"easingCurve": "OutBack"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 350,
							"direction": "vertical",
							"easingCurve": "OutBack"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
							"ajustesBoton": "ajustePagina",
							"acercaDeBoton": "acercaDePagina",
							"ayudaBoton": "ayudaPagina"
						}
					]
				}
			]
		},
		{	
			"name":"derechoPaginas",
			"transitionAnimation":[
				{
					"fade":[
						{
							"active": true,
							"duration":500,
							"easingCurve": "OutBack"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 500,
							"direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
							"perfilBoton": "perfilPagina",
							"botonMenuEliminar": "eliminarPagina",
							"masBoton": "masPagina"
						}
					]
				}
			]
		}
	],
	"QPushButtonGroup": [{
			"Buttons": [
				"inicioBoton",
				"estudianteBoton",
				"materiaBoton",
				"profesorBoton",
				"notaBoton",
				"documentoBoton",
				"gradosBoton"
			],
			"Style":[{
				"Active": "border-left: 2px solid #f00;font-weight;bold;",
				"NotActive": "transparent;"
			}]
		},
		{
		"Buttons": [
			"ajustesBoton",
			"acercaDeBoton",
			"ayudaBoton"
			],
			"Style":[{
				"Active": "border-left: 2px solid #f00;font-weight;bold;",
				"NotActive": "transparent;"
			}]
		}
	],
	"QCard": [{
		"cards": [
				"carta1",
				"carta2",
				"carta3",
				"carta4"
			],
			"shadow":[{
				"color": "#555",
				"blurRadius": 20,
				"xOffset": 0,
				"yOffset": 0
			}]
		},
		{
		"cards": [
			"widget_8",
			"widget_9",
			"card7",
			"card8"
		],
		"shadow":[{
				"color": "#555",
				"blurRadius": 20,
				"xOffset": 0,
				"yOffset": 0
			}]
		}
	]
}
